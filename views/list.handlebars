<!-- this is where we will put the list of saved content items -->

<legend>My Favorites</legend>
<br>
<div class='container'>
  <div class='jumbotron'>

    {{#if currentUser}}
    {{#each currentUser.likedContent}}
    <br>
    <div class='row'>
      <p><b>{{this.mood}}</b></p>
      <p>{{this.resource}}</p>
      <p>{{this.link}}</p>
    </div>
    <br>
    {{/each}}
    {{/if}}

  </div>
</div>